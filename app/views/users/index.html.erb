<h1>Adnat</h1>
<h3>Log in</h3>
<%= form_with scope: :user, url: users_path, local: true do |form| %>
    <p>
        <%= form.label :email %><br>
        <%= form.text_field :email %>
    </p>

    <p>
        <%= form.label :password %><br>
        <%= form.text_field :password %>
    </p>
    
    <p>
        <%= form.submit :Log_in %>
    </p>
<% end %>
<%= link_to 'Sign up', new_user_path %>